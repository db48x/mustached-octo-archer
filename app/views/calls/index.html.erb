<h1>Listing calls</h1>

<table>
  <tr>
    <th>Start</th>
    <th>End</th>
    <th>Frequency</th>
    <th>Talkgroup</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @calls.each do |call| %>
  <tr>
    <td><%= call.start %></td>
    <td><%= call.end %></td>
    <td><%= call.frequency %></td>
    <td><%= call.group_name %></td>
    <td><%= link_to 'Show', call %></td>
    <td><%= link_to 'Edit', edit_call_path(call) %></td>
    <td><%= link_to 'Destroy', call, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Call', new_call_path %>
